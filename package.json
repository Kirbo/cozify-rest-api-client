{
  "name": "cozify-rest-api-client",
  "version": "0.0.1",
  "description": "Cozify REST API Node.js client",
  "repository": "git@github.com:kirbo/cozify-rest-api-client.git",
  "author": "Kimmo Saari <kirbo@kirbo-designs.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node example.ts",
    "lint": "tslint -c tslint.js src/*.ts src/**/*.ts",
    "test": "yarn test:debug --silent",
    "test:debug": "cross-env NODE_ENV=test jest --verbose --forceExit --detectOpenHandles"
  },
  "engines": {
    "node": ">=8.10.0",
    "yarn": ">=1.5.0"
  },
  "main": "dist/index.js",
  "bin": {
    "cozify-api": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "Cozify"
  ],
  "dependencies": {
    "axios": "0.18.0",
    "bluebird": "3.5.1",
    "fs-extra": "7.0.0",
    "qs": "6.5.2"
  },
  "devDependencies": {
    "@types/fs-extra": "5.0.4",
    "@types/jest": "23.3.1",
    "@types/node": "10.9.2",
    "@types/qs": "6.5.1",
    "cross-env": "5.2.0",
    "jest": "23.5.0",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.1"
  }
}
